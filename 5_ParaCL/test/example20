a = 5;
b = 6;
a + b;
c = { 
    i = 6;
    b = { 
        x = 0;
        while (1) {
            ++x;
            if (x == 6)
                return x;
        }
    }
    while (i > 0) {
        --i;
        ++b;
    }
    b + 1;
}
print c;
t = { x = 5; y = 10; x + y; }
print t;